{
    "sample-project": {
      "project_id": "sample-project-id",
      "region": "us-central1",
      "firewall_rules": [
        {
          "name": "allow-ssh-ingress",
          "direction": "INGRESS",
          "source_ranges": ["203.0.113.0/24"],
          "destination_ranges": [],
          "target_tags": ["ssh-access"],
          "allowed": [
            {
              "protocol": "tcp",
              "ports": ["22"]
            }
          ]
        }
      ]
    },
    "firewall_rules": [
        {
          "name": "allow-egress-http",
          "direction": "EGRESS",
          "source_ranges": [],
          "destination_ranges": ["0.0.0.0/0"],
          "target_tags": ["web-server"],
          "allowed": [
            {
              "protocol": "tcp",
              "ports": ["80", "443"]
            }
          ]
        }
    ]
}